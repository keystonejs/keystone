{
  "name": "@keystone-6/example-graphql-ts-gql",
  "version": null,
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "keystone dev",
    "start": "keystone start",
    "build": "keystone build && ts-gql build",
    "postinstall": "keystone postinstall && ts-gql build"
  },
  "dependencies": {
    "@keystone-6/core": "^6.4.0",
    "@prisma/client": "6.5.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@ts-gql/compiler": "^0.16.7",
    "@ts-gql/eslint-plugin": "^0.9.1",
    "@ts-gql/tag": "^0.7.3",
    "eslint": "^8.0.0",
    "prisma": "6.5.0",
    "typescript": "^5.5.0"
  },
  "ts-gql": {
    "schema": "schema.graphql",
    "mode": "no-transform",
    "addTypename": false
  },
  "eslintConfig": {
    "plugins": [
      "@ts-gql"
    ],
    "rules": {
      "@ts-gql/ts-gql": "error"
    }
  }
}
