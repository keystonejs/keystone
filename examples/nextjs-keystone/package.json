{
  "name": "@keystone-6/example-nextjs-keystone",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "keystone:dev": "cross-env PUBLIC_URL=http://localhost:3000 keystone dev",
    "keystone:build": "cross-env PUBLIC_URL=http://localhost:3000 keystone build",
    "keystone:start": "cross-env PUBLIC_URL=http://localhost:3000 keystone start",
    "next:dev": "cross-env PUBLIC_URL=http://localhost:4000 next dev -p 4000",
    "next:build": "cross-env PUBLIC_URL=http://localhost:3000 next build",
    "build": "yarn keystone:build && yarn next build",
    "start": "cross-env PUBLIC_URL=http://localhost:3000 next start"
  },
  "dependencies": {
    "@keystone-6/auth": "^6.0.0",
    "@keystone-6/core": "^4.0.1",
    "@preconstruct/next": "^4.0.0",
    "cross-env": "^7.0.3",
    "graphql-request": "^5.0.0",
    "graphql-upload": "^15.0.2",
    "graphql-yoga": "^3.1.0",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.14",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "typescript": "~4.9.4"
  },
  "repository": "https://github.com/keystonejs/keystone/tree/main/examples/nextjs-keystone"
}
